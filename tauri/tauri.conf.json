{
  "productName": "filesync",
  "version": "0.7.12",
  "identifier": "com.filesync.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build && npm run export ",
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "FileSync",
        "width": 1000,
        "height": 800
      }
    ],
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": []
      },
      "csp": {
        "assets": "http://asset.localhost"
      }
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all",
    "createUpdaterArtifacts": "v1Compatible"
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "plugins": {
    "sql": {
      "preload": ["sqlite:filesync.db"]
    }
  }
}
