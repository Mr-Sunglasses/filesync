{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build && npm run export ",
    "frontendDist": "../out",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all",
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "filesync",
  "version": "0.7.10",
  "identifier": "com.filesync.app",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "FileSync",
        "width": 1000,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  }
}